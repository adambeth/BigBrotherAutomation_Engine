<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Ping Test Suite">
    <listeners>
        <listener class-name="API.baseTest.reportBuilder.ListenerResults"></listener>
        <listener class-name="API.baseTest.reportBuilder.ISuiteListener"></listener>
    </listeners>
    <test verbose="2" preserve-order="true"
          name="Basic Ping Test">
        <classes>
            <class name="API.mwmTests.PING.basicPingTest">
                <methods>
                    <include name="basicPingTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Ping LookUp Test" verbose="2" preserve-order="true">
        <classes>
            <class name="API.mwmTests.PING.lookUpTest">
                <methods>
                    <include name="simpleVendTestLookUp"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>