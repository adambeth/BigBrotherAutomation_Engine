<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Flow Manager Get Client Tests">
    <listeners>
        <listener class-name="baseTestUtils.baseTest.reportBuilder.ListenerResults"></listener>
        <listener class-name="baseTestUtils.baseTest.reportBuilder.ISuiteListener"></listener>
    </listeners>
    <test verbose="2" preserve-order="true"
          name="Get All Client CLIENT Test.java">
        <classes>
            <class name="API.FlowManagerServices.management.getAllClientCLIENTTest">
                <methods>
                    <include name="getallCLIENTS"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="Get All Clients VENDORTest">
        <classes>
            <class name="API.FlowManagerServices.management.getAllClientsVENDORTest">
                <methods>
                    <include name="getallCLIENTS"/>
                </methods>
            </class>
        </classes>
    </test>

    <test verbose="2" preserve-order="true"
          name="get All Clients FUNDINGSOURCE Test">
        <classes>
            <class name="API.FlowManagerServices.management.getAllClientsFUNDINGSOURCETest">
                <methods>
                    <include name="getAllFundingSources"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>