<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Get Keywords Test Suite" thread-count="5" parallel="methods">
    <listeners>
        <listener class-name="api.testUtilities.reportBuilder.ListenerResults"></listener>
        <listener class-name="api.testUtilities.reportBuilder.ISuiteListener"></listener>
    </listeners>
    <test verbose="2" preserve-order="true"
          name="Get Client Keyword Test">
        <classes>
            <class name="API.FlowManagerServices.keywords.getClientKeyWordsTest">
                <methods>
                    <include name="getClientKeyWordsTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Get FUNDING SOURCE Keyword Test">
        <classes>
            <class name="API.FlowManagerServices.keywords.getFUNDINGSOURCEKeyWordTest">
                <methods>
                    <include name="getFUNDINGSOURCEKeyWordTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Get Vendors Keyword Test">
        <classes>
            <class name="API.FlowManagerServices.keywords.getVENDORKeyWordsTest">
                <methods>
                    <include name="getVENDORKeyWordsTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>