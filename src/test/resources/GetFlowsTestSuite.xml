<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Get Flows Test Suite" thread-count="5" parallel="methods">
    <listeners>
        <listener class-name="api.testUtilities.reportBuilder.ListenerResults"></listener>
        <listener class-name="api.testUtilities.reportBuilder.ISuiteListener"></listener>
    </listeners>

    <test verbose="2" preserve-order="true"
          name="get VENDOR FlowName">
        <classes>
            <class name="API.FlowManagerServices.flows.getVENDORFlowName">
                <methods>
                    <include name="getVendorFlowNameTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="get Client FlowName">
        <classes>
            <class name="API.FlowManagerServices.flows.getClientFlowName">
                <methods>
                    <include name="getClientFlowNameTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="get Funding Source Flow Steps">
        <classes>
            <class name="API.FlowManagerServices.flows.getFundingSourceFlowSteps">
                <methods>
                    <include name="getFundingSourceFlowStepsTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="get Client Flow Steps">
        <classes>
            <class name="API.FlowManagerServices.flows.getClientFlowSteps">
                <methods>
                    <include name="getClientFlowStepsTest"/>
                </methods>
            </class>
        </classes>
    </test>

    <test verbose="2" preserve-order="true"
          name="Get Flows Single By FUNDINGSOURCE">
        <classes>
            <class name="API.FlowManagerServices.flows.getFlowsSingleByFUNDINGSOURCE">
                <methods>
                    <include name="getFlowSingByFundingSource"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="Get Flows Single By VENDOR">
        <classes>
            <class name="API.FlowManagerServices.flows.getFlowSingleByVENDOR">
                <methods>
                    <include name="getFlowSingByVendorTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="get Flow Single By Client Test">
        <classes>
            <class name="API.FlowManagerServices.flows.getFlowSingByClientTest">
                <methods>
                    <include name="getFlowSingByClientTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="Get flows by Vendor">
        <classes>
            <class name="API.FlowManagerServices.flows.getFlowsByVendorTest">
                <methods>
                    <include name="getFlowsbyVendor"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="Get flows By Client">
        <classes>
            <class name="API.FlowManagerServices.flows.getFlowByCLIENTTest">
                <methods>
                    <include name="getFlowsbyClient"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="Get flows by Funding Source">
        <classes>
            <class name="API.FlowManagerServices.flows.getFlowsByFUNDINGSOURCETest">
                <methods>
                    <include name="getFlowsByFundingSource"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>