<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">

    <listeners>
        <listener class-name="baseTest.reportBuilder.ListenerResults"></listener>
        <listener class-name="baseTest.reportBuilder.ISuiteListener"></listener>
    </listeners>

    <test verbose="2" preserve-order="true"
          name="Get Flows Single By FUNDINGSOURCE">
        <classes>
            <class name="FlowManagerServices.flows.getFlowsSingleByFUNDINGSOURCE">
                <methods>
                    <include name="getFlowSingByFundingSource"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="Get Flows Single By VENDOR">
        <classes>
            <class name="FlowManagerServices.flows.getFlowSingleByVENDOR">
                <methods>
                    <include name="getFlowSingByVendorTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="get Flow Sing By Client Test">
        <classes>
            <class name="FlowManagerServices.flows.getFlowSingByClientTest">
                <methods>
                    <include name="getFlowSingByClientTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="Get flows by Vendor">
        <classes>
            <class name="FlowManagerServices.flows.getFlowsByVendorTest">
                <methods>
                    <include name="getFlowsbyVendor"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="Get flows By Client">
        <classes>
            <class name="FlowManagerServices.flows.getFlowByCLIENTTest">
                <methods>
                    <include name="getFlowsbyClient"/>
                </methods>
            </class>
        </classes>
    </test>
    <test verbose="2" preserve-order="true"
          name="Get flows by Funding Source">
        <classes>
            <class name="FlowManagerServices.flows.getFlowsByFUNDINGSOURCETest">
                <methods>
                    <include name="getFlowsByFundingSource"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>