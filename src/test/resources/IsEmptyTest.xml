<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <listeners>
        <listener class-name="baseTest.reportBuilder.ListenerResults"></listener>
    <listener class-name="baseTest.reportBuilder.ISuiteListener"></listener>
    </listeners>
    <test verbose="2" preserve-order="true"
          name="is Empty Simple Null Test">
        <classes>
            <class name="USSDTests.isEmptyTests.isEmptySimpleNullTest">
                <methods>
                    <include name="ussdIsEmptySimpleNullTest"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="isEmpty Simple Test">
        <classes>

            <class name="USSDTests.isEmptyTests.isEmptySimpleTest">
                <methods>
                    <include name="ussdIsEmptySimpleNullTest"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>